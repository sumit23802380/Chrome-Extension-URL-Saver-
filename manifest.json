{
    "name" : "URL Saver",
    "description" :"Extension that allows you to save the data which show if you click on extension and also on Android Application",
    "version" : "1.0",
    "manifest_version" : 3,
    "background" :{
        "service_worker" : "background.js"
    },
    "action" :{
        "default_popup" : "popup.html"
    },
    "icons" :{
        "16" : "/images/16.png",
        "32" : "/images/32.png",
        "64" : "/images/64.png"
    },
    "permissions" : [
        "tabs"
    ]
}